<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".ExpressTPVApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ActionBarOrange"
        tools:targetApi="31">
        <activity
            android:name=".ui.view.CobrosActivity"
            android:theme="@style/ActionBarOrange"
            android:parentActivityName=".ui.view.VentasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.ListaCategoriasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.CategoriaEditorActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.ArticuloEditorActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.ListaArticulosActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view.VentasActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>